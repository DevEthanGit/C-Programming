3.
#!/bin/bash

# Start Ticker 5 in the background
./ticker.sh 5 &

# Start Ticker 7 in the foreground
./ticker.sh 7

# Suspend Ticker 7
CTRL+Z

# List the jobs
jobs

# Activate Ticker 7 in the background
bg %7

# Bring Ticker 5 into the foreground
fg %5

# Suspend Ticker 5
CTRL+Z

# Kill Ticker 5
kill %5

# Kill Ticker 7
kill %7
4.
gcc -o ticker ticker.c
#compile with a basic name
./ticker 3
